@model ViewModels.Models.CategoryVm<div class="col-7 px-0" style="background-color: #f7f7f7;  height: auto;box-shadow: 4px 5px 5px #6c757d;">    <div class="page-top" style="">               <div class="lockIcon mt-4">            <img id="mainicon" src="~/images/fries.png" style="height: 60px;" alt="">        </div>        <p class="mt-2"> Categories Page</p>    </div>    <div class="container mb-4" style="display:flex;justify-content:center">         @if (TempData["error"] != null)                {                    <span class="text-danger">@TempData["error"]</span>                }        <form class="mt-5 formm" id="formm" >                       <div class="mb-3">                <select name="All Categories" id="cars" class="data">                    <option value="">All Categories</option>                                  </select>            </div>            <div class="mb-3">                <label for="categoryName" class="form-label">                    <p class="formInput">Add Category <span style="color: red;">*</span></p>                </label>                <input type="text" class="form-control data" id="" aria-describedby="" placeholder="Add New Category" asp-for="name">                          </div>            <div class="mb-3" style="display:flex;justify-content:center">                <button type="" value="submit" class="btn submit"                        style="background-color: red;color: white;border-radius: 7px;">                   Add                </button>            </div>                       <hr>            <div>            </div>                   </form>            </div>   </div><script>    $(document).ready(function(){                $("#mainicon").mouseenter(function () {            $("#mainicon").attr('src', '/images/pizza.png');        })        $("#mainicon").mouseleave(function () {            $("#mainicon").attr('src', '/images/fries.png');        })    })</script>