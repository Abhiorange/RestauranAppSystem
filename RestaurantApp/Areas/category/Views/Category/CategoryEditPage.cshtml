@model ViewModels.Models.CategoryEditVm<body>    <div class="container mt-2 overflow-tableEntry">        <div class="col-12 bg-light border p-2">            Edit        </div>        <div class="border">            <form method="post" asp-action="GetCategoryById" asp-controller="Category" class="mx-2">                <div class="col-12 d-flex flex-column my-2">                    <label for="title">CategoryName</label>                    <input type="text" id="title" class="form-control" asp-for="categoryname">                    <span class="text-danger" asp-validation-for="categoryname"></span>                </div>                <input asp-for="Id" type="text" hidden />                <div class="col-4 d-flex flex-column my-2">                    <label for="status">Stocks</label>                    <input type="text" id="title" class="form-control" asp-for="stocks">                    <span class="text-danger" asp-validation-for="stocks"></span>                </div>                <div class="col-4 d-flex flex-column my-2">                    <label for="status">IsActive</label>                    <select class="form-select dropdown-toggle" id="status" asp-for="isActive">                        <option value="1" selected="@Model.isActive.Equals(1)">Active</option>                        <option value="0" selected="@Model.isActive.Equals(0)">Inactive</option>                    </select>                </div>                              <div class="d-flex justify-content-end">                    <button type="button" class="btn btn-secondary rounded-pill"> <a class="text-white text-decoration-none rounded-pill me-1">Cancel</a></button>                    <button type="submit" class="btn btn-outline-warning rounded-pill">Submit</button>                </div>            </form>        </div>    </div>    @section Scripts{        @{            <partial name="_ValidationScriptsPartial" />        }    }</body>